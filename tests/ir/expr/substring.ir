@main def main() = {
  let str = "0123456789"
  
  assert (= (substring str 0 1) "0")
  assert (= (substring str 0 10) "0123456789")
  assert (= (substring str 4 6) "45")
  assert (= (substring str 0f 1f) "0")
  assert (= (substring str 0f 10f) "0123456789")
  assert (= (substring str 4f 6f) "45")
}
